<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rtm.com.sysendpoint" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/tecnimotor"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Activitys.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name="rtm.com.sysendpoint.Activitys.MenuActivity"
            android:label="Menu"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name="rtm.com.sysendpoint.Activitys.LoginActivity"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name="rtm.com.sysendpoint.Activitys.SignupActivity"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name="rtm.com.sysendpoint.Activitys.ClienteActivity"
            android:theme="@style/Base.AppTheme">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>

        </activity>

    </application>

</manifest>
